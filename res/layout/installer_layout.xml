<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearLayout2"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_gravity="fill_horizontal|fill_vertical">
    <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/imageView1" android:src="@drawable/cowbell" android:layout_gravity="center|right" android:layout_marginRight="45dip" android:layout_marginTop="65dip"></ImageView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="@string/moreCowbell"
		android:textColor="#000000" android:id="@+id/textView1" android:layout_gravity="center|center_horizontal|top" android:layout_marginTop="35dip"></TextView>
</LinearLayout>
